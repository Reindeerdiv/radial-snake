{
  "name": "radial-snake",
  "description": "A tutorial for creating a Tron-style game",
  "private": true,
  "scripts": {
    "serve": "npm run parse:fonts && nodemon server.js",
    "parse:fonts": "node helpers/font_parser.js",
    "compile:cpp": "emcc -O1 --pre-js resources/cpp/pre.js --post-js resources/cpp/post.js --bind -o resources/cpp/dist/cpp.bundle.js resources/cpp/src/index.cpp"
  },
  "dependencies": {
    "async": "^2.1.4",
    "boom": "^4.2.0",
    "hapi": "^16.0.1",
    "inert": "^4.0.3",
    "underscore": "^1.8.3",
    "xmldom": "^0.1.27"
  }
}
